{
  "name": "ember-flexberry",
  "version": "2.6.0-beta.1",
  "description": "Ember Flexberry Addon",
  "license": "MIT",
  "author": {
    "name": "Flexberry Team",
    "email": "mail@flexberry.net",
    "url": "http://flexberry.net/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Flexberry/ember-flexberry.git"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "lint:ts": "eslint --ext .ts --env node .",
    "start": "ember server",
    "test": "ember try:testall"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --no-ignore",
    "*.ts": "eslint --no-ignore --ext .ts --env node",
    "*.hbs": "ember-template-lint --config '{ \"extends\": [\"recommended\", \"stylistic\"], \"rules\": { \"no-bare-strings\": true } }'"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^2.4.2",
    "dexie": "^1.4.2",
    "ember-ajax": "0.7.1",
    "ember-block-slots": "1.1.3",
    "ember-browserify": "1.1.9",
    "ember-cli": "2.4.3",
    "ember-cli-app-version": "^1.0.0",
    "ember-cli-dependency-checker": "^1.2.0",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-inject-live-reload": "^1.4.0",
    "ember-cli-less": "^1.5.4",
    "ember-cli-moment-shim": "2.2.1",
    "ember-cli-qunit": "^1.4.0",
    "ember-cli-release": "0.2.8",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-data": "2.4.3",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^1.0.5",
    "ember-link-action": "0.0.34",
    "ember-load-initializers": "^0.5.1",
    "ember-moment": "6.0.0",
    "ember-resolver": "^2.0.3",
    "ember-sinon": "^2.2.0",
    "ember-template-lint": "^2.14.0",
    "ember-try": "^0.2.2",
    "eslint": "^7.13.0",
    "eslint-plugin-ember": "^9.6.0",
    "eslint-plugin-hbs": "^1.0.0",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-qunit": "^5.0.0",
    "husky": "^4.3.0",
    "inflection": "1.10.0",
    "lint-staged": "^10.5.1",
    "loader.js": "^4.0.1",
    "moment": "2.11.2",
    "moment-timezone": "0.5.0",
    "node-uuid": "^1.4.7",
    "npm-run-all": "^4.1.5",
    "semantic-ui-ember": "git://github.com/Flexberry/Semantic-UI-Ember.git#version-0.9.3",
    "ui-ember-slider": "0.5.0"
  },
  "dependencies": {
    "ember-cli-babel": "^5.1.6",
    "ember-cli-htmlbars": "^1.0.3",
    "ember-flexberry-data": "~2.6.0-beta.0",
    "ember-i18n": "4.2.0",
    "ember-test-selectors": "2.1.0",
    "ember-truth-helpers": "1.2.0",
    "ember-validations": "~2.0.0-alpha.4",
    "lodash": "^4.4.0",
    "strip-bom": "^2.0.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "keywords": [
    "ember-addon",
    "flexberry"
  ]
}
