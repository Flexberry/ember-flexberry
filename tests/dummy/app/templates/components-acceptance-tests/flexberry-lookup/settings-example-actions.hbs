{{flexberry-lookup
  componentName=componentName
  placeholder=placeholder
  readonly=readonly
  value=model.type
  projection=projection
  displayAttributeName=displayAttributeName
  title=title
  relatedModel=model
  relationName=relationName
  choose="showLookupDialog"
  remove="removeLookupValue"
  autocomplete=autocomplete
  dropdown=dropdown
  chooseText=chooseText
  removeText=removeText
  chooseButtonClass=chooseButtonClass
  removeButtonClass=removeButtonClass
  lookupLimitPredicate=lookupLimitPredicate
  choose=(action "externalChooseAction")
  remove=(action "externalRemoveAction")
  updateLookupValue=(action 'updateLookupValue')
}}