<h3 class="ui header">{{t "forms.components-examples.flexberry-groupedit.settings-example.caption"}}</h3>
<form class="ui form flexberry-vertical-form" role="form">
  <div class="field">
    {{flexberry-groupedit
      componentName="components-examples/flexberry-groupedit/settings-example/aggregator-details-flexberry-groupedit"
      class="attached"
      modelProjection=modelProjection.attributes.details
      content=model.details
      readonly=readonly
      orderable=false
      rowClickable=false
      headerClickable=false
      showCheckBoxInRow=true
      showDeleteButtonInRow=true
    }}
  </div>
</form>
