{{flexberry-error error=error}}
<h3 class="ui header">{{t "forms.components-examples.flexberry-text-cell.settings-example.caption"}}</h3>
<form class="ui form flexberry-vertical-form" role="form">
  <div class="field">
    {{#settings-example
      controllerProperties=this
      componentSettingsMetadata=componentSettingsMetadata
      componentTemplateText=componentTemplateText
      componentBlockOverflow="visible"
    }}
    {{flexberry-text-cell
      value=value
      maxTextLength=maxTextLength
      cutBySpaces=cutBySpaces
    }}
    {{/settings-example}}
  </div>
</form>
