{{flexberry-error error=error}}
<h3>{{t 'forms.ember-flexberry-dummy-multi-list.caption'}}</h3>
<div class="row">
  {{flexberry-objectlistview
    modelName="ember-flexberry-dummy-application-user"
    modelProjection=multiListSettings.MultiUserList.modelProjection
    editFormRoute=multiListSettings.MultiUserList.editFormRoute
    content=model.MultiUserList
    createNewButton=true
    refreshButton=true
    sorting=multiListSettings.MultiUserList.computedSorting
    orderable=true
    sortByColumn=(action "sortByColumn")
    addColumnToSorting=(action "addColumnToSorting")
    beforeDeleteAllRecords=(action "beforeDeleteAllRecords")
    pages=multiListSettings.MultiUserList.pages
    perPageValue=multiListSettings.MultiUserList.perPageValue
    perPageValues=multiListSettings.MultiUserList.perPageValues
    recordsTotalCount=multiListSettings.MultiUserList.recordsTotalCount
    hasPreviousPage=multiListSettings.MultiUserList.hasPreviousPage
    hasNextPage=multiListSettings.MultiUserList.hasNextPage
    previousPage=(action "previousPage")
    gotoPage=(action "gotoPage")
    nextPage=(action "nextPage")
    componentName="MultiUserList"
  }}
</div>
