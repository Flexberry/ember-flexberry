<div class="row">
  {{flexberry-objectlistview
    editFormRoute=multiListSettings.MultiSuggestionList.editFormRoute
    showCheckBoxInRow=true
    modelName="ember-flexberry-dummy-suggestion"
    modelProjection=multiListSettings.MultiSuggestionList.modelProjection
    content=model.MultiSuggestionList
    createNewButton=true
    enableFilters=true
    filters=multiListSettings.MultiSuggestionList.filters
    refreshButton=true
    exportExcelButton=true
    sorting=multiListSettings.MultiSuggestionList.computedSorting
    orderable=true
    sortByColumn=(action "sortByColumn")
    addColumnToSorting=(action "addColumnToSorting")
    beforeDeleteAllRecords=(action "beforeDeleteAllRecords")
    applyFilters=(action "applyFilters")
    resetFilters=(action "resetFilters")
    pages=multiListSettings.MultiSuggestionList.pages
    perPageValue=multiListSettings.MultiSuggestionList.perPageValue
    perPageValues=multiListSettings.MultiSuggestionList.perPageValues
    recordsTotalCount=multiListSettings.MultiSuggestionList.recordsTotalCount
    hasPreviousPage=multiListSettings.MultiSuggestionList.hasPreviousPage
    hasNextPage=multiListSettings.MultiSuggestionList.hasNextPage
    previousPage=(action "previousPage")
    gotoPage=(action "gotoPage")
    nextPage=(action "nextPage")
    componentName="MultiSuggestionList"
    showDeleteMenuItemInRow=true
    deleteButton=true
  }}
</div>
<div class="row">
  {{flexberry-objectlistview
    content=model.MultiHierarchyList
    modelName="ember-flexberry-dummy-suggestion-type"
    modelProjection=multiListSettings.MultiHierarchyList.modelProjection
    editFormRoute=multiListSettings.MultiHierarchyList.editFormRoute
    orderable=false
    componentName="MultiHierarchyList"
    beforeDeleteAllRecords=(action "beforeDeleteAllRecords")
    colsConfigButton=false
    disableHierarchicalMode=false
    showCheckBoxInRow=true
    pages=multiListSettings.MultiHierarchyList.pages
    perPageValue=multiListSettings.MultiHierarchyList.perPageValue
    perPageValues=multiListSettings.MultiHierarchyList.perPageValues
    recordsTotalCount=multiListSettings.MultiHierarchyList.recordsTotalCount
    hasPreviousPage=multiListSettings.MultiHierarchyList.hasPreviousPage
    hasNextPage=multiListSettings.MultiHierarchyList.hasNextPage
    previousPage=(action "previousPage")
    gotoPage=(action "gotoPage")
    nextPage=(action "nextPage")
    availableCollExpandMode=true
    inHierarchicalMode=multiListSettings.MultiHierarchyList.inHierarchicalMode
    hierarchicalAttribute=multiListSettings.MultiHierarchyList.hierarchicalAttribute
    inExpandMode=multiListSettings.MultiHierarchyList.inExpandMode
  }}
</div>
