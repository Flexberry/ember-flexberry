{{#if sidepageFilesIsOpen}}
{{#modal-dialog
    title=(t "forms.ember-flexberry-dummy-suggestion-edit.files-caption")
    useSidePageMode=true
    useOkButton=false
    useCloseButton=false
    close=(action "closeSidepageFiles")
}}
{{flexberry-groupedit
    componentName="filesGroupEdit"
    content=model.files
    mainModelProjection=modelProjection
    modelProjection=modelProjection.attributes.files
    orderable=true
    readonly=readonly
}}
{{/modal-dialog}}
{{/if}}